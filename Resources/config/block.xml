<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>

        <parameter key="integrated_website.block_handler.contact.class">Integrated\Bundle\WebsiteBundle\Block\ContactBlockHandler</parameter>

    </parameters>

    <services>

        <service id="integrated_website.block_handler.contact" class="%integrated_website.block_handler.contact.class%">
            <argument type="service" id="form.factory" />
            <argument type="service" id="request_stack" />
            <tag name="integrated.block" type="contact" />
        </service>

    </services>
</container>